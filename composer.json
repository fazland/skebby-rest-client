{
    "name": "fazland/skebby-rest-client",
    "description": "Unofficial Skebby Rest Client",
    "type": "library",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@fazland.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "giggsey/libphonenumber-for-php": "^7.0|^8.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/options-resolver": "^2.7|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "kcs/function-mock": "dev-master",
        "php-http/discovery": "^1.0",
        "php-http/guzzle6-adapter": "^1.1,>=1.1.1",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.4",
        "php-http/message-factory": "^1.0",
        "phpunit/phpunit": "^6.5",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\SkebbyRestClient\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\SkebbyRestClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "license": "MIT"
}
